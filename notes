shell commands:
    ./convert.py | grep "{v}" | grep -v "\"\"" | wc -l  # how many with derivatives
    ./convert.py | grep "{v} " | grep "\"\""  # check what verbs found no derivatives
    ./convert.py | g "{v}" | wc -l  # how many verbs
    grep -o "{.*}" es-en.dic | sort| uniq  # show tags


